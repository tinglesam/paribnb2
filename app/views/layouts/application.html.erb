</head>
<body>
<br>
<br>
<br>


  <%= render partial:"layouts/navbar" %>

  <div id="flash">
    <% flash.each do |key, value| -%>
      <div id="flash_<%= key %>"><%=h value %></div>
    <% end %>
  </div>

  <%= yield %>

</body>
</html>
